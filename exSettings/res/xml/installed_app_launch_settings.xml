<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/application_info_label"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/app_launch_domain_links_title" android:key="app_launch_domain_links">
        <com.android.exsettings.DropDownPreference android:persistent="false" android:title="@string/app_launch_open_domain_urls_title" android:key="app_link_state" />
        <com.android.exsettings.applications.AppDomainsPreference android:persistent="false" android:title="@string/app_launch_supported_domain_urls_title" android:key="app_launch_supported_domain_urls" android:dependency="app_link_state" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/app_launch_other_defaults_title" android:key="app_launch_other_defaults">
        <com.android.exsettings.applications.ClearDefaultsPreference android:persistent="false" android:key="app_launch_clear_defaults" />
    </PreferenceCategory>
</PreferenceScreen>